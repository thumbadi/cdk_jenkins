select c.internal_claim_num as "INTERNAL_CLAIM_NUM", c.xref_internal_claim_num as "XREF_INTERNAL_CLAIM_NUM", c.received_dt as "PROCESS_DATE", 
c.src_claim_type_cd as "SRC_CLAIM_TYPE_CODE", c.medicare_src_of_coverage as "MEDICARE_SOURCE_OF_COVERAGE", c.srvc_dt as "DATE OF SERVICE (DOS)", 
c.rx_srvc_ref_num as "PRESCRIPTION_SERVICE_REFERENCE_NO", null "FILL_NUMBER", c.ncpdp_id as "NCPDP_ID", c.srvc_npi_num as "SERVICE_PROVIDER_ID", 
c.prescriber_id as "PRESCRIBER_ID", c.ndc_cd as "PRODUCT_SERVICE_ID", c.quantity_dispensed as "QUANTITY_DISPENSED", c.days_supply  as "DAYS_SUPPLY", 
c.indicator_340b_yn as "340B_INDICATOR", c.orig_submitting_contract_num as "CONTRACT_NUMBER", b.wac_amt as "WAC_Per_Unit_Price", b.mfp_amt as "MFP_Per_Unit",
b.sdra_amt as "Standard_Default_Refund_Amount(SDRA)", a.pymt_pref as "Service_Provider_Payment_Method_Preference",
null as "Previous_MFP_Refund_Paid_Product/Service_Identifier", null as "Previous_MFP_Refund_Paid_Amount", null as "Previous_MFP_Refund_Paid_Date", 
null as "Previous_MFP_Refund_Paid_Quantity", null as "Previous_MFP_refund_Paid_Method", null as "MRA_ERROR_CODE_1", null as "MRA_ERROR_CODE_2", null as "MRA_ERROR_CODE_3",
null as "MRA_ERROR_CODE_4", null as "MRA_ERROR_CODE_5", null as "MRA_ERROR_CODE_6", null as "MRA_ERROR_CODE_7", null as "MRA_ERROR_CODE_8", null as "MRA_ERROR_CODE_9", 
null as "MRA_ERROR_CODE_10", null as "MRA_PM_SWITCH", null as "Method_for_Determining_MFP_Refund_Amount", null as "Quantity_of_Selected_Drug", null as "Amount_of_Funds_Authorized", 
null as "Amount_of_Funds_Requested", null as "Total_MFP_Refund_Amount", null as "MFP_Refund_Adjustment (Yes or No)", null as "MFP_Refund_Transmission_Date_and_Time",
d.mfr_id as "Manufacturer_ID", d.mfr_name as "Manufacturer_Name"
from claim.mtf_claim c join claim.mtf_claim_de_tpse a on a.received_dt = c.received_dt and a.received_id = c.received_id
     join  claim.mtf_claim_pricing b on b.received_dt = c.received_dt and b.received_id = c.received_id
     join  claim.mtf_claim_manufacturer d on d.received_dt = c.received_dt and d.received_id = c.received_id
where c.mtf_curr_claim_stus_ref_cd = 'MRN';
