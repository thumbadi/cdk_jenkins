CREATE TABLE de_tpse.de_tpse_payee_dtl (
	org_type varchar(20) NULL,
	de_id int8 NULL,
	tpse_id int8 NULL,
	payee_id varchar(20) NULL,
	org_name varchar(200) NULL,
	org_ftin_num varchar(10) NULL,
	org_npi_num varchar(10) NULL,
	profit_yn varchar(2) NULL,
	pymt_pref_cd varchar(3) NULL,
	pymt_routing_num varchar(9) NULL,
	pymt_acct_num varchar(20) NULL,
	pymt_acct_type varchar(20) NULL,
	eff_dt date NULL,
	end_dt date NULL,
	pymt_adrs_cd varchar(100) NULL,
	pymt_adrs_line_1 varchar(100) NULL,
	pymt_adrs_line_2 varchar(100) NULL,
	pymt_adrs_city varchar(100) NULL,
	pymt_adrs_state varchar(2) NULL,
	pymt_adrs_zip_code varchar(10) NULL,
	contact_first_name varchar(200) NULL,
	contact_last_name varchar(200) NULL,
	contact_title varchar(100) NULL,
	contact_phone_num_1 varchar(20) NULL,
	contact_phone_num_2 varchar(20) NULL,
	contact_email_adrs varchar(100) NULL,
	refresh_ts timestamp NULL,
	part_load_yn bool NULL,
	xfr_req_yn bool NULL
);

