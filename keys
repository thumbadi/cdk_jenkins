CREATE TABLE claim.mtf_claim_mra (
	mra_received_dt date DEFAULT CURRENT_DATE NOT NULL,
	mra_received_id int8 DEFAULT nextval('claim.mra_received_id_seq'::regclass) NOT NULL,
	claim_received_dt date NOT NULL,
	claim_received_id int8 NOT NULL,
	claim_file_metadata_id int8 NOT NULL,
	internal_claim_num varchar(30) NOT NULL,
	mra_mtfpm_yn bool NULL,
	mra_rfnd_calc_mthd_cd varchar(3) NULL,
	mra_rfnd_amt numeric(38, 2) NULL,
	mra_rfnd_ts timestamp(6) NULL,
	mra_stus_cd varchar(3) NULL,
	mra_error_cd_1 varchar(3) NULL,
	mra_error_cd_2 varchar(3) NULL,
	mra_error_cd_3 varchar(3) NULL,
	mra_error_cd_4 varchar(3) NULL,
	mra_error_cd_5 varchar(3) NULL,
	mra_error_cd_6 varchar(3) NULL,
	mra_error_cd_7 varchar(3) NULL,
	mra_error_cd_8 varchar(3) NULL,
	mra_error_cd_9 varchar(3) NULL,
	mra_error_cd_10 varchar(3) NULL,
	insert_user_id int8 NOT NULL,
	insert_ts timestamp(6) DEFAULT CURRENT_TIMESTAMP NOT NULL,
	update_user_id int8 NULL,
	update_ts timestamp(6) NULL,
)
