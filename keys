CREATE TABLE shared.prvdr_ncpdp (
	prvdr_ncpdp_id varchar(7) NOT NULL,
	prvdr_ncpdp_dba_name varchar(60) NULL,
	prvdr_ncpdp_lgl_busns_name varchar(60) NULL,
	prvdr_ncpdp_store_num varchar(10) NULL,
	prvdr_ncpdp_line_1_adr varchar(55) NULL,
	prvdr_ncpdp_line_2_adr varchar(55) NULL,
	prvdr_ncpdp_invld_plc_name varchar(30) NULL,
	prvdr_ncpdp_invld_state_cd varchar(2) NULL,
	prvdr_ncpdp_invld_zip_cd varchar(9) NULL,
	prvdr_ncpdp_invld_cnty_cd varchar(5) NULL,
	geo_sk numeric(38) NULL,
	geo_zip4_cd varchar(4) NULL,
	prvdr_ncpdp_site_tel_num varchar(10) NULL,
	prvdr_ncpdp_site_extnsn_num varchar(5) NULL,
	prvdr_ncpdp_site_fax_num varchar(10) NULL,
	prvdr_ncpdp_site_email_adr varchar(50) NULL,
	prvdr_ncpdp_store_open_dt date NULL,
	prvdr_ncpdp_store_clsr_dt date NULL,
	prvdr_ncpdp_mlg_line_1_adr varchar(55) NULL,
	prvdr_ncpdp_mlg_line_2_adr varchar(55) NULL,
	prvdr_ncpdp_invld_mlg_plc_name varchar(30) NULL,
	prvdr_ncpdp_invld_mlg_state_cd varchar(2) NULL,
	prvdr_ncpdp_invld_mlg_zip_cd varchar(9) NULL,
	geo_mlg_sk numeric(38) NULL,
	geo_mlg_zip4_cd varchar(4) NULL,
	prvdr_ncpdp_cntct_1st_name varchar(20) NULL,
	prvdr_ncpdp_cntct_mdl_name varchar(1) NULL,
	prvdr_ncpdp_cntct_last_name varchar(20) NULL,
	prvdr_ncpdp_cntct_title_name varchar(30) NULL,
	prvdr_ncpdp_cntct_tel_num varchar(10) NULL,
	prvdr_ncpdp_cntct_extnsn_num varchar(5) NULL,
	prvdr_ncpdp_cntct_email_adr varchar(50) NULL,
	prvdr_ncpdp_dspnsr_cls_cd varchar(2) NULL,
	prvdr_ncpdp_1_dspnsr_type_cd varchar(2) NULL,
	prvdr_ncpdp_2_dspnsr_type_cd varchar(2) NULL,
	prvdr_ncpdp_3_dspnsr_type_cd varchar(2) NULL,
	prvdr_ncpdp_upin_id varchar(10) NULL,
	prvdr_ncpdp_npi varchar(10) NULL,
	prvdr_ncpdp_fed_tax_num varchar(15) NULL,
	prvdr_ncpdp_state_lcns_num varchar(20) NULL,
	prvdr_ncpdp_state_tax_num varchar(15) NULL,
	prvdr_ncpdp_dlt_dt date NULL,
	prvdr_ncpdp_hstry_vrsn_dt date NULL,
	prvdr_ncpdp_deactvtn_cd varchar(2) NULL,
	prvdr_ncpdp_physn_name varchar(60) NULL,
	prvdr_ncpdp_reinstmt_cd varchar(2) NULL,
	prvdr_ncpdp_reinstmt_dt date NULL,
	prvdr_ncpdp_stus_340b_cd varchar(2) NULL,
	prvdr_ncpdp_stus_340b_sw varchar(1) NULL,
	insert_user_id int8 NOT NULL,
	insert_ts timestamp(6) DEFAULT CURRENT_TIMESTAMP NOT NULL,
	update_user_id int8 NULL,
	update_ts timestamp(6) NULL,
	CONSTRAINT prvdr_ncpdp_pk PRIMARY KEY (prvdr_ncpdp_id)
);
