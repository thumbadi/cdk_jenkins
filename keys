CREATE TABLE mfr.mfr_bank_file_vw (
    recordoperation VARCHAR(1),
    organizationcode VARCHAR(10),
    payeeid VARCHAR(20),
    organizationidentifier VARCHAR(20),
    organizationname VARCHAR(100),
    organizationlegalname VARCHAR(100),
    organizationtin VARCHAR(15),
    organizationtintype VARCHAR(10),
    profit_nonprofit CHAR(1),
    organizationnpi VARCHAR(20),
    paymentmode VARCHAR(10),
    routingtransitnumber VARCHAR(9),
    accountnumber VARCHAR(20),
    accounttype VARCHAR(20),
    effectivestartdate DATE,
    effectiveenddate DATE,
    addresscode VARCHAR(20),
    addressline1 VARCHAR(100),
    addressline2 VARCHAR(100),
    cityname VARCHAR(50),
    state VARCHAR(2),
    postalcode VARCHAR(10),
    contactcode VARCHAR(20),
    contactfirstname VARCHAR(50),
    contactlastname VARCHAR(50),
    contacttitle VARCHAR(50),
    contactphone VARCHAR(20),
    contactfax VARCHAR(20),
    contactotherphone VARCHAR(20),
    contactemail VARCHAR(100)
);


